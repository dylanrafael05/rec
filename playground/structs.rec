struct Vector2
{
    x f32,
    y f32,
}

fn struct_test() Vector2
{
    return new Vector2
    {
        x = 0.1 cast(f32),
        y = 0.2 cast(f32),
    };
}

fn sqr_magnitude(value *Vector2) f32
{
    return (value*.x * value*.x) + (value*.y * value*.y);
}

fn break_test() f32
{
    let test = struct_test();
    break test
    {
        let x = x,
        let y = y,
    };

    return x;
}