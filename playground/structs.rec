struct Vector2
{
    x f32,
    y f32,
}

fn struct_test() Vector2
{
    return new Vector2
    {
        x = 0.1,
        y = 0.2,
    };
}

fn sqr_magnitude(value &Vector2) f32
{
    return (value*.x * value*.x) + (value*.y * value*.y);
}

fn t1() f32
{
    let s = struct_test();
    return sqr_magnitude(s&);
}

fn break_test() f32
{
    let test = struct_test();
    break test
    {
        let x = x,
        let y = y,
    };

    return x;
}