mod math

struct Vector2
{
    x f32,
    y f32
}

mod as Vector2

fn make(x f32, y f32) Vector2
{
    return new Vector2
    {
        x = x,
        y = y
    };
}

fn get_x(&self) Vector2
{
    return Vector2::make(self*.x, self*.x);
}

fn get_px(&self) &f32
{
    return self*.y&;
}

fn get_arr(&self) [f32]
{
    return [self*.x, self*.y];
}